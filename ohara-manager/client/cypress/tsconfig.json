{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es5",
    "module": "esnext",
    "strict": true,
    "lib": ["es5", "dom", "es2017"],
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "jsx": "react",
    "allowJs": true,
    "types": ["cypress", "@types/testing-library__cypress"]
  },
  "include": ["**/*.ts"]
}
